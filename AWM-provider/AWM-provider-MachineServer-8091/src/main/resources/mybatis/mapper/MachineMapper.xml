<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ttmy.awm.dao.MachineMapper">

    <select id="queryAll" resultType="machine">
       select * from washingmachine
    </select>

    <select id="quaryMachineByID" parameterType="String" resultType="machine">
        select * from washingmachine where machine_id = #{id}
    </select>
</mapper>